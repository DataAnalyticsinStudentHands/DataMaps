{
  "name": "dash_data_maps",
  "version": "1.0.0",
  "description": "This is a development repository.",
  "main": "index.js",
  "dependencies": {
    "ftps": "^0.4.6",
    "highcharts": "^4.2.5",
    "junk": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-meteor": "^3.4.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "meteor": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb/base",
      "plugin:meteor/guide"
    ],
    "globals": {
      "chokidar": true,
      "Nodemailer": true,
      "moment": true,
      "logger": true,
      "Highcharts": true,
      "Papa": true,
      "LiveData": true,
      "AggrData": true,
      "Sites": true,
			"Exports": true,
      "DataExporter": true,
      "channelHash": true,
      "flagsHash": true,
      "unitsHash": true
    },
    "rules": {
      "max-len": [
        2,
        120,
        2,
        {
          "ignoreUrls": true,
          "ignoreComments": false
        }
      ],
			"arrow-body-style": ["error", "always"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DataAnalyticsinStudentHands/DataMaps.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DataAnalyticsinStudentHands/DataMaps/issues"
